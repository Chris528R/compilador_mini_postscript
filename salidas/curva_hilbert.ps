paso   avanzar
prev paso   hilbertB
1   girar
paso   avanzar
prev paso   hilbertA
1 neg   girar
} if
  end
} def

/hilbertA {
  20 dict begin
  /paso exch def
  /nivel exch def
nivel 0 gt
{
